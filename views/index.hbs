  <title>Human Line - Vanteo</title>
</head>

<body>
  {{>navbar data=details}}

  {{>slider slides=slides}}

{{!-- banner-1 section --}}
<section class="banner1_section py-5 background1">
  <div class="comtainer text-center">
    <div class="row">
      <div class="col-md-12 ">
        <h1 class=" text-secondary">Rehabilitation services and physical therapy !!</h1>
        <p>Outpatient department (OPD), Surgical department, Inpatient service (IP), Nursing department, Physical medicine, Paramedical department, and Rehabilitation department, Dietary department, Pharmacy department, Operation theater complex (OT), Radiology department (X-ray)</p>
        <button class="btn btn-outline-primary">Register and claim now !</button>

      </div>
    </div>
  </div>
</section>


  {{!-- services section --}}

  <section id="services_section" class="py-4">
    <div class="container">

      <h1 class="text-center text-secondary">Our sevices</h1>
      <div class="row mt-5">

        {{!-- first service col --}}
        {{#each services}}
        <div class="col-md-4">

          <div class="card">
            <div class="card-body">
              <div class="container text-center py-3">
                <i class="{{this.icon}} service_icon text-secondary"></i>
              </div>
              <h5 class="card-title">{{this.title}}</h5>
              <p>{{this.description}}</p>

              <a href="{{this.link}}">{{this.linkText}}</a>
              <a href="#" class="ml-3">Support</a>
            </div>

          </div>
        </div>
        {{/each}}
      </div>
    </div>
  </section>


{{!-- banner-2 section --}}
<div class="banner2_seaction py-5">
  <div class="container text-center">
    <div class="row">
    {{!-- left-side --}}
      <div class="col-md-6 text-center pt-5">
        <h3>We serve for humanity , not for money</h3>
        <p>Employee-driven, collaborative, and social in nature, human apps celebrate deep connections, diversity, and individuality. And they are inclusive, open, and accessible to all.</p>
        <button class="btn btn-primary">Click Here</button>
      </div>
      {{!-- right-side --}}
      <div class="col-md-6 py-2">
            <img  src="/static/images/graphic1.svg" style="height:300px" alt="">
      </div>
          </div>

  </div>
</div>


{{!-- banner-3 section --}}
<div class="banner2_seaction py-5">
  <div class="container text-center">
    <div class="row">

        {{!-- left-side --}}
      <div class="col-md-6 py-2">
            <img  src="/static/images/graphic2.svg" class="img-fluid" style="height:300px" alt="">
      </div>
    {{!-- right-side --}}
      <div class="col-md-6 text-center pt-5">
        <h3>We serve for humanity , not for money</h3>
        <p>Employee-driven, collaborative, and social in nature, human apps celebrate deep connections, diversity, and individuality. And they are inclusive, open, and accessible to all.</p>
        <button class="btn btn-primary">Click Here</button>
      </div>
    
          </div>

  </div>
</div>




  {{!-- contact us section --}}
  <section id="Contact_us_section" class="py-5 background1">
    <div class="container">
      <h3 class="text-center  text-secondary">Contact Us</h3>
      <div class="row">
        <div class="col-md-12">

          <form action="/process-contact-form" method="post">
            <div class="mb-3">
              <label for="exampleInputEmail1" class="form-label">Your Email address</label>
              <input name="email" type="email" class="form-control" id="exampleInputEmail1" aria-describedby="emailHelp">
              <div id="emailHelp" class="form-text">We'll never share your email with anyone else.</div>
            </div>


            <div class="mb-3">
              <label for="contactNumberField" class="form-label">Your contact number</label>
              <input name="phone" type="number" class="form-control" id="contactNumberField">
            </div>

            <div class="form-floating my-2 ">
              <textarea name="query" class="form-control" placeholder="Enter your querry here" id="queryField"
                style="height: 150px"></textarea>
              <label for="queryField">Your query here</label>
            </div>

            <div class="container text-center"> 
              <button type="submit" class="btn btn-dark">Submit</button>
            </div>

          </form>






        </div>
      </div>
    </div>

  </section>
{{!-- about section --}}
<section class="about_section py-5" >
  <div class="container text-center">
